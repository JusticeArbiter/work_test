-- contains
SELECT '' AS five, t1.f1
   FROM TINTERVAL_TBL t1
   WHERE not t1.f1 <<
        tinterval '["Aug 15 14:23:19 1980" "Sep 16 14:23:19 1990"]'
   ORDER BY t1.f1;
 five |                         f1                          
------+-----------------------------------------------------
      | ["1983-09-04 23:59:12+08" "1983-10-04 23:59:12+08"]
      | ["1990-02-15 12:15:03+08" "2001-09-23 11:12:13+08"]
      | ["1947-05-10 23:59:12+08" "1973-01-14 03:14:21+08"]
(3 rows)

-- make time interval
SELECT '' AS three, t1.f1
   FROM TINTERVAL_TBL t1
   WHERE t1.f1 &&
        (abstime 'Aug 15 14:23:19 1983' <#>
         abstime 'Sep 16 14:23:19 1983')
   ORDER BY t1.f1;
 three |                         f1                          
-------+-----------------------------------------------------
       | ["-infinity" "infinity"]
       | ["1983-09-04 23:59:12+08" "1983-10-04 23:59:12+08"]
       | ["1970-01-01 08:00:00+08" "1995-05-01 00:30:30+08"]
(3 rows)

