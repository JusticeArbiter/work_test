--
-- TINTERVAL
--
CREATE TABLE TINTERVAL_TBL (f1  tinterval);
CREATE TABLE
-- Should accept any abstime,
-- so do not bother with extensive testing of values
INSERT INTO TINTERVAL_TBL (f1)
   VALUES ('["-infinity" "infinity"]');
INSERT 0 1
