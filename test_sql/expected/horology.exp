SELECT '' AS ten, ABSTIME_TBL.f1 AS abstime, RELTIME_TBL.f1 AS reltime
    FROM ABSTIME_TBL, RELTIME_TBL
   WHERE (ABSTIME_TBL.f1 + RELTIME_TBL.f1) < abstime 'Jan 14 14:00:00 1971'
   ORDER BY abstime, reltime;
 ten |           abstime            |  reltime  
-----+------------------------------+-----------
     | Sat May 10 23:59:12 1947 CST | -00:00:14
     | Sat May 10 23:59:12 1947 CST | 00:01:00
     | Sat May 10 23:59:12 1947 CST | 05:00:00
     | Sat May 10 23:59:12 1947 CST | 10 days
     | Sat May 10 23:59:12 1947 CST | 3 mons
     | Thu Jan 01 08:00:00 1970 CST | -00:00:14
     | Thu Jan 01 08:00:00 1970 CST | 00:01:00
     | Thu Jan 01 08:00:00 1970 CST | 05:00:00
     | Thu Jan 01 08:00:00 1970 CST | 10 days
     | Thu Jan 01 08:00:00 1970 CST | 3 mons
(10 rows)

-- these four queries should return the same answer
-- the "infinity" and "-infinity" tuples in ABSTIME_TBL cannot be added and
-- therefore, should not show up in the results.
SELECT '' AS three, * FROM ABSTIME_TBL
  WHERE  (ABSTIME_TBL.f1 + reltime '@ 3 year')         -- +3 years
    < abstime 'Jan 14 14:00:00 1977';
 three |              f1              
-------+------------------------------
       | Sun Jan 14 03:14:21 1973 CST
       | Thu Jan 01 08:00:00 1970 CST
       | Sat May 10 23:59:12 1947 CST
(3 rows)

SELECT '' AS three, * FROM ABSTIME_TBL
   WHERE  (ABSTIME_TBL.f1 + reltime '@ 3 year ago')    -- -3 years
     < abstime 'Jan 14 14:00:00 1971';
